{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "unifiedjs.vscode-mdx"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "accessibility.signals.diffLineDeleted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineInserted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineModified": {
    "sound": "off"
  },
  "accessibility.signals.lineHasBreakpoint": {
    "sound": "off"
  },
  "accessibility.signals.lineHasError": {
    "sound": "off"
  },
  "accessibility.signals.lineHasFoldedArea": {
    "sound": "off"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "accessibility.signals.noInlayHints": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellCompleted": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellFailed": {
    "sound": "off"
  },
  "accessibility.signals.onDebugBreak": {
    "sound": "off"
  },
  "accessibility.signals.progress": {
    "sound": "off"
  },
  "accessibility.signals.taskCompleted": {
    "sound": "off"
  },
  "accessibility.signals.taskFailed": {
    "sound": "off"
  },
  "accessibility.signals.terminalCommandFailed": {
    "sound": "off"
  },
  "accessibility.signals.terminalQuickFix": {
    "sound": "off"
  },
  "apc.stylesheet": {
    ".ghost-text-decoration,.ghost-text-decoration-preview": {
      "font-family": "Monaspace Krypton",
      "font-weight": "lighter"
    },
    ".mtki": {
      "font-family": "Monaspace Radon",
      "font-weight": "lighter"
    }
  },
  "chat.editor.fontFamily": "'Monaspace Krypton Var', Consolas, 'Courier New', monospace",
  "debug.disassemblyView.showSourceCode": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.codeLens": false,
  "editor.codeLensFontFamily": "'Monaspace Krypton Var', Consolas, 'Courier New', monospace",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Monaspace Neon Var', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.inlayHints.fontFamily": "'Monaspace Krypton Var', Consolas, 'Courier New', monospace",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": []
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "explorer.confirmDelete": false,
  "files.associations": {
    "development": "plaintext",
    "production": "plaintext",
    "staging": "plaintext"
  },
  "git.autofetch": true,
  "git.fetchOnPull": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.editor.enableCodeActions": true,
  "github.copilot.enable": {
    "*": "true"
  },
  "githubPullRequests.pullBranch": "never",
  "importCost.bundleSizeDecoration": "minified",
  "importCost.fontStyle": "italic",
  "importCost.javascriptExtensions": ["\\.jsx?$"],
  "importCost.typescriptExtensions": ["\\.tsx?$"],
  "playwright.reuseBrowser": false,
  "prisma.showPrismaDataPlatformNotification": false,
  "redhat.telemetry.enabled": false,
  "scm.inputFontFamily": "editor",
  "search.followSymlinks": false,
  "search.useIgnoreFiles": false,
  "todohighlight.isEnable": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Snazzy Plus - Darker w/ Italics",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "fixed",
  "workbench.iconTheme": "chalice-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": true
}
