[user]
	name = Kelly Mears
	email = developers@tinypixel.dev
[credential]
helper = osxkeychain
[alias]
wow = log --all --graph --decorate --oneline --simplify-by-decoration
[core]
	excludesfile = /Users/kellymears/.gitignore_global
[color]
branch = auto
diff = auto
status = auto
[color "branch"]
current = yellow reverse
local = yellow
remote = green
[color "diff"]
meta = yellow bold
frag = magenta bold
old = red bold
new = green bold
[color "status"]
added = yellow
changed = green
untracked = cyan
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
